			<th style="width: 232px; font-size: 14px; padding-top: 5px; padding-bottom: 5px; text-align: center;">HOBBIES & SPECIAL INTERESTS</th>
		</tr>
		{% set comp_skills = [] %}
		{% for skills in object.getSkills %}{% for skill in skills.getComputer %}{% set comp_skills = comp_skills|merge([skill]) %}{% endfor %}{% endfor %}
		{% set cs_len = comp_skills|length %}

		{% set rela_skills = [] %}
		{% for skills in object.getSkills %}{% for skill in skills.getRelated %}{% set rela_skills = rela_skills|merge([skill]) %}{% endfor %}{% endfor %}
		{% set rl_len = rela_skills|length %}

		{% set othr_skills = [] %}
		{% for skills in object.getSkills %}{% for skill in skills.getHobbies %}{% set othr_skills = othr_skills|merge([skill]) %}{% endfor %}{% endfor %}
		{% set hb_len = othr_skills|length %}

		{% set totals = [cs_len, rl_len, hb_len] %}
		{% set x = max(totals) %}

		{% for i in range(0, x-1) %}
			<tr>
				<td align="center">{{comp_skills[i]|default('')}}</td>
				<td align="center">{{rela_skills[i]|default('')}}</td>
				<td align="center">{{othr_skills[i]|default('')}}</td>
			</tr>
		{% endfor %}
	</table>
</page>
